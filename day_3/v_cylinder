def fib0_seq(n):
    fib_series=[]
    a,b=0,1
    for _ in range(n):
        fib_series.append(a)
        a,b=b,a+b
    return fib_series
n=int(input("enter the nuimber upto nth term"))
if n<=0:
    print(input("please enter the positive integers"))
else:
    print(f"the fibonacci series up to nth term is:{fib0_seq(n)}")    